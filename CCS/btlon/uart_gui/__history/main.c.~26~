#include <16F877a.h>
#FUSES NOWDT, HS, PUT, NOPROTECT,
#use DELAY(clock=11059200)
#use rs232(baud=9600,xmit=pin_c6,rcv=pin_c7) 
#DEFINE ON PIN_A0
#DEFINE OFF PIN_A1
int8 TDATA;
void main()
{
SET_TRIS_A(0XFF);
SET_TRIS_D(0x00); 
TDATA = 0X00;
while(true)
{ 
PUTC(TDATA);   
OUTPUT_D(TDATA);
DELAY_MS(100);
TDATA++;    
}
}
